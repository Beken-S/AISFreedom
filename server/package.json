{
  "private": true,
  "name": "AISFreedom-server",
  "version": "0.1.0",
  "scripts": {
    "preparation": "node ./scripts/preparation",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "serve": "node build/server",
    "start": "npm run preparation && npm run clean && npm run build && npm run serve",
    "dev": "npm run preparation && npm run clean && npm run build && concurrently -k -c 'blue,green' --names 'TS,API' -p '[{name}]' npm:dev:build npm:dev:serve",
    "dev:build": "tsc -p tsconfig-dev.json",
    "dev:serve": "nodemon ./build/server development",
    "lint": "eslint src --ext .js --ext .ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/minimist": "^1.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.14",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "sql-log-prettifier": "^0.1.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.14.2",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "minimist": "^1.2.6",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "puppeteer": "^17.1.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.4",
    "sequelize-typescript": "^2.1.3",
    "uuid": "^8.3.2"
  }
}
